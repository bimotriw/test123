<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Define the background properties like color etc -->
    <item android:id="@android:id/background">
        <shape
            android:innerRadius="@dimen/oustlayout_dimen16"
            android:shape="ring"
            android:useLevel="true"
            android:thicknessRatio="24.0">
            <solid android:color="@color/LiteGray" />
        </shape>
    </item>

    <!-- Define the progress properties like start color, end color etc -->
    <item android:id="@android:id/progress">
        <shape
            android:innerRadius="@dimen/oustlayout_dimen15"
            android:shape="ring"
            android:useLevel="true"
            android:thicknessRatio="30.0">
            <gradient
                android:startColor="@color/LiteGray"
                android:centerColor="@color/LiteGray"
                android:centerY="0.5"
                android:endColor="@color/LiteGray"
                android:angle="0"
                />
            <!--<solid android:color="@color/Orange" />-->
        </shape>
    </item>
</layer-list>