<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    android:id="@+id/parentpopupLayout">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mainpopup_animlayout"
        android:padding="@dimen/oustlayout_dimen1"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/White"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout8">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/oustlayout_dimen35"
                        android:padding="@dimen/oustlayout_dimen10"
                        android:id="@+id/closeLayout" >
                        <ImageButton
                            android:layout_width="@dimen/oustlayout_dimen20"
                            android:layout_height="@dimen/oustlayout_dimen20"
                            android:id="@+id/btnClose"
                            android:layout_gravity="center_horizontal|right"
                            android:background="@android:color/transparent"
                            android:src="@drawable/ic_close_img"
                            android:scaleType="fitXY"
                            android:tint="@color/Black"
                            android:contentDescription="@string/app_name" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/challengerLayout"
                        android:layout_marginTop="@dimen/oustlayout_dimen20">
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center">
                            <ImageView
                                android:layout_width="@dimen/oustlayout_dimen55"
                                android:layout_height="@dimen/oustlayout_dimen55"
                                android:id="@+id/userIcon"
                                android:layout_gravity="center"/>
                        </RelativeLayout>
                    </LinearLayout>
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/oustlayout_dimen8">
                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:id="@+id/headerescription"
                        android:lineSpacingExtra="@dimen/oustlayout_dimen5"
                        android:text=""
                        android:textColor="@color/Black"
                        android:gravity="center|center_horizontal"
                        android:textSize="@dimen/ousttext_dimen18"
                        android:maxLines="10"
                        android:paddingLeft="@dimen/oustlayout_dimen10"
                        android:paddingRight="@dimen/oustlayout_dimen10"/>

                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/passwordLayout"
                    android:layout_marginTop="@dimen/oustlayout_dimen8"
                    android:paddingRight="@dimen/oustlayout_dimen10"
                    android:paddingLeft="@dimen/oustlayout_dimen10">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:id="@+id/passwordTxtLayout"
                            android:layout_alignParentBottom="false"
                            android:layout_alignParentTop="true">
                            <com.oustme.oustsdk.customviews.CustomTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:color/transparent"
                                android:id="@+id/alertDescription"
                                android:lineSpacingExtra="@dimen/oustlayout_dimen4"
                                android:text=""
                                android:textColor="@color/Black"
                                android:gravity="center"
                                android:scrollbars="vertical"
                                android:textSize="@dimen/ousttext_dimen15"
                                android:maxLines="10" />
                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:visibility="gone"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/layout_survey_coins"
                    android:layout_marginTop="@dimen/oustlayout_dimen8"
                    android:paddingRight="@dimen/oustlayout_dimen10"
                    android:paddingLeft="@dimen/oustlayout_dimen10">
                    <LinearLayout
                        android:background="@android:color/transparent"
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <com.oustme.oustsdk.customviews.CustomTextView
                            android:layout_gravity="center"
                            android:textColor="@color/Black"
                            android:lineSpacingExtra="@dimen/oustlayout_dimen5"
                            android:textSize="@dimen/oustlayout_dimen20"
                            android:gravity="center_vertical|center_horizontal"
                            android:id="@+id/txt_survet_hardcoded"
                            android:text="You earned "
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <ImageView
                            android:id="@+id/coins_icon"
                            android:padding="@dimen/oustlayout_dimen10"
                            android:src="@drawable/coins_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="50dp"/>

                        <com.oustme.oustsdk.customviews.CustomTextView
                            android:textColor="@color/Black"
                            android:layout_gravity="center"
                            android:lineSpacingExtra="@dimen/oustlayout_dimen5"
                            android:textSize="@dimen/oustlayout_dimen22"
                            android:gravity="center_vertical|center_horizontal"
                            android:id="@+id/txt_survey_coins"
                            tools:text="500"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </LinearLayout>
                </LinearLayout>

                <RelativeLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/oustlayout_dimen55"
                    android:layout_marginTop="@dimen/oustlayout_dimen20"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/signInLayout">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/alertbtn_layout"
                        android:background="@color/LiteGreen">
                        <com.oustme.oustsdk.customviews.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/oustlayout_dimen25"
                            android:layout_marginTop="@dimen/oustlayout_dimen20"
                            android:id="@+id/btnAlert"
                            android:layout_centerHorizontal="true"
                            android:text=""
                            android:singleLine="true"
                            android:gravity="center"
                            android:background="@drawable/white_cornerback"
                            android:textSize="@dimen/ousttext_dimen15"
                            android:textColor="@color/White"
                            android:paddingRight="@dimen/oustlayout_dimen20"
                            android:paddingLeft="@dimen/oustlayout_dimen20"/>
                    </RelativeLayout>
                    <ImageView
                        android:layout_width="@dimen/oustlayout_dimen60"
                        android:layout_height="@dimen/oustlayout_dimen50"
                        android:scaleType="fitXY"
                        android:background="@drawable/ic_arrow_drop_downwhite"
                        android:layout_centerHorizontal="true" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
