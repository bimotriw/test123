<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/fulltransparent"
    android:layout_margin="@dimen/oustlayout_dimen20"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_marginRight="@dimen/oustlayout_dimen8"
        android:layout_marginLeft="@dimen/oustlayout_dimen8"
        android:layout_height="match_parent"
        android:id="@+id/card_mainrow"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.oustme.oustsdk.customviews.NewCurveLayout
                android:layout_width="@dimen/oustlayout_dimen100"
                android:layout_height="@dimen/oustlayout_dimen100"
                android:gravity="center"
                app:curveviewwidth="7dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/LiteGray"
                    android:padding="@dimen/oustlayout_dimen1">
                    <com.oustme.oustsdk.customviews.NewCurveLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:curveviewwidth="7dp">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@color/white_pressed">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/card_learningimage"
                                android:visibility="gone"
                                android:layout_centerVertical="true" />
                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/card_image"
                                android:visibility="visible"
                                android:scaleType="centerCrop"
                                android:layout_centerVertical="true" />
                            <ImageView
                                android:layout_width="@dimen/oustlayout_dimen23"
                                android:layout_height="@dimen/oustlayout_dimen23"
                                android:id="@+id/card_videoicon"
                                android:layout_centerInParent="true"
                                android:visibility="gone"
                                android:tint="@color/Gray"/>
                        </RelativeLayout>
                    </com.oustme.oustsdk.customviews.NewCurveLayout>
                </RelativeLayout>
            </com.oustme.oustsdk.customviews.NewCurveLayout>
            <!--<com.oustme.oustsdk.customviews.CurvedRelativeLayout-->
                <!--android:layout_width="@dimen/oustlayout_dimen100"-->
                <!--android:layout_height="@dimen/oustlayout_dimen100"-->
                <!--android:gravity="center"-->
                <!--android:background="@drawable/oustroundedcorner_background"-->
                <!--app:curveviewwidth="7dp">-->
                <!--<com.oustme.oustsdk.customviews.CustomRoundedImageView-->
