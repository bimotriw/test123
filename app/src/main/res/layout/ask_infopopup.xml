<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/popupBackGround"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="@dimen/oustlayout_dimen1"
            android:layout_centerInParent="true"
            android:layout_marginLeft="@dimen/oustlayout_dimen20"
            android:layout_marginRight="@dimen/oustlayout_dimen20">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:id="@+id/infopopup_animlayout"
                android:background="@color/White"  >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="@dimen/oustlayout_dimen40"
                        android:layout_height="@dimen/oustlayout_dimen40"
                        android:layout_alignParentRight="true"
                        android:id="@+id/closeLayout">
                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/btnClose"
                            android:layout_gravity="right"
                            android:src="@drawable/ic_close_img"
                            android:tint="@color/Black"
                            android:background="@android:color/transparent"
                            android:layout_marginTop="@dimen/oustlayout_dimen5"
                            android:contentDescription="@string/app_name" />
                    </LinearLayout>
                    <RelativeLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="@dimen/oustlayout_dimen35" >
                        <com.oustme.oustsdk.customviews.CustomTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@android:color/transparent"
                            android:id="@+id/txtRejectChallengeMsg"
                            android:textColor="@color/DarkGray"
                            android:gravity="left|center_vertical"
                            android:textSize="@dimen/ousttext_dimen18"
                            android:lineSpacingExtra="@dimen/oustlayout_dimen5"
                            android:layout_marginLeft="@dimen/oustlayout_dimen20"
                            android:layout_marginRight="@dimen/oustlayout_dimen20"
                            android:text=""
                            android:paddingTop="@dimen/oustlayout_dimen2" />

                    </RelativeLayout>
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/oustlayout_dimen30"
                    android:layout_marginLeft="@dimen/oustlayout_dimen18"
                    android:layout_marginRight="@dimen/oustlayout_dimen15"
                    android:id="@+id/boxlayout"
                    android:visibility="gone">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:id="@+id/deletevideo_checkbox"
                        android:checked="false"
                        android:gravity="center"
                        android:tint="@color/LiteGreen"
                        android:src="@drawable/ic_check_box_green"/>
                    <com.oustme.oustsdk.customviews.CustomTextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textColor="@color/DarkGray"
                        android:textSize="@dimen/ousttext_dimen13"
                        android:gravity="left|center_vertical"
                        android:id="@+id/deletevideo_text"
                        android:text="delete video after level complete"
                        android:layout_alignTop="@+id/deletevideo_checkbox"
                        android:layout_toRightOf="@+id/deletevideo_checkbox" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/oustlayout_dimen40"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/yesOrNoLayout"
                    android:layout_marginLeft="@dimen/oustlayout_dimen15"
                    android:layout_marginRight="@dimen/oustlayout_dimen15"
                    android:layout_marginTop="@dimen/oustlayout_dimen15"
                    android:layout_marginBottom="@dimen/oustlayout_dimen15">
                    <com.oustme.oustsdk.customviews.CustomOustButton
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/btnNo"
                        android:layout_weight="1"
                        android:layout_marginRight="@dimen/oustlayout_dimen5"
                        android:layout_marginLeft="@dimen/oustlayout_dimen5"
                        android:gravity="center|center_vertical"
                        android:textColor="@color/White"
                        android:textSize="@dimen/ousttext_dimen14"
                        android:singleLine="true"
                        android:background="@color/Orange"
                        android:paddingTop="@dimen/oustlayout_dimen2" />
                    <com.oustme.oustsdk.customviews.CustomOustButton
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/btnYes"
                        android:layout_marginLeft="@dimen/oustlayout_dimen5"
                        android:layout_marginRight="@dimen/oustlayout_dimen5"
                        android:layout_gravity="center_horizontal"
                        android:singleLine="true"
                        android:textSize="@dimen/ousttext_dimen14"
                        android:textColor="@color/White"
                        android:background="@color/LiteGreen"
                        android:paddingTop="@dimen/oustlayout_dimen2" />

                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>