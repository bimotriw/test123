<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:id="@+id/fab_base_layout"
    android:padding="5dp"
    android:layout_marginRight="5dp"
    android:layout_height="match_parent">

    <com.oustme.oustsdk.customviews.FloatingActionsMenu
        android:id="@+id/multiple_actions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/oustlayout_dimen60"
        android:layout_marginEnd="7dp"
        android:layout_marginRight="5dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        fab:fab_addButtonColorNormal="@color/LiteGreen"
        fab:fab_colorPressed="@color/LiteGreen"
        fab:fab_addButtonColorPressed="@color/LiteGreen"
        fab:fab_labelStyle="@style/menu_labels_style"
        android:layout_gravity="end">
        <com.oustme.oustsdk.customviews.FloatingActionButton
            android:id="@+id/fab_action_joinmeeting"
            fab:fab_title="gf"
            fab:fab_size="mini"
            android:src="@drawable/joinmeeting_landingicon"
            android:padding="@dimen/oustlayout_dimen10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_colorNormal="@color/LiteGreen"
            fab:fab_colorPressed="@color/LiteGreen"/>

        <com.oustme.oustsdk.customviews.FloatingActionButton
            android:id="@+id/fab_action_leaderboard"
            fab:fab_size="mini"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_colorNormal="@color/LiteGreen"
            fab:fab_colorPressed="@color/LiteGreen"/>
        <com.oustme.oustsdk.customviews.FloatingActionButton
            android:id="@+id/action_favourite"
            fab:fab_title=""
            fab:fab_size="mini"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_colorNormal="@color/LiteGreen"
            fab:fab_colorPressed="@color/LiteGreen"/>

    </com.oustme.oustsdk.customviews.FloatingActionsMenu>


</RelativeLayout>
