<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_city_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/DarkGray_a"
    android:descendantFocusability="blocksDescendants">

    <LinearLayout
        app:layout_constraintTop_toTopOf="parent"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <com.oustme.oustsdk.customviews.LightCustomTextView
        android:background="@color/whitea"
        android:layout_marginBottom="@dimen/oustlayout_dimen1"
        android:gravity="center_vertical"
        android:id="@+id/textViewCity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:focusable="false"
        android:padding="@dimen/oustlayout_dimen12"
        android:text="Text"
        android:textColor="@drawable/list_view_text_color_change"
        android:textSize="@dimen/ousttext_dimen18"
        tools:ignore="MissingConstraints" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>