<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="@dimen/oustlayout_dimen30"
        android:layout_height="@dimen/oustlayout_dimen30"
        android:padding="@dimen/oustlayout_dimen1"
        android:id="@+id/charBack">
        <com.oustme.oustsdk.customviews.GameTextView
            android:id="@+id/jumbletext"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/empty_text"
            android:textAllCaps="true"
            android:textSize="@dimen/ousttext_dimen16"
            android:textColor="@color/Black"
            android:textStyle="bold"
            android:gravity="center"/>
    </RelativeLayout>

</LinearLayout>