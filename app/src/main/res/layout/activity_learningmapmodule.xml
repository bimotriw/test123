<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/transparent">

        <RelativeLayout
            android:id="@+id/animinit_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/popupBackGroundd"
            android:visibility="visible">

            <FrameLayout
                android:id="@+id/percentageScreen"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/whitea"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/start_backgroundimage"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="fitXY"
                        android:visibility="visible" />

                    <ImageView
                        android:id="@+id/start_backgroundimage_downloaded"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="fitXY"
                        android:visibility="gone" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/popupBackGrounde"></RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/maindownload_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/oustlayout_dimen220"
                            android:layout_centerInParent="true"
                            android:layout_marginBottom="@dimen/oustlayout_dimen40"
                            android:orientation="vertical"
                            android:visibility="visible">

                            <RelativeLayout
                                android:id="@+id/learningcard_downloadprogresslayout"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/oustlayout_dimen130"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/learningcard_downloadprogressimage"
                                    android:layout_width="@dimen/oustlayout_dimen100"
                                    android:layout_height="@dimen/oustlayout_dimen100"
                                    android:layout_centerInParent="true"
                                    android:background="@drawable/orange_round_textview"
                                    android:visibility="visible" />

                                <ImageView
                                    android:id="@+id/learningcard_downloadprogressimagea"
                                    android:layout_width="@dimen/oustlayout_dimen100"
                                    android:layout_height="@dimen/oustlayout_dimen100"
                                    android:layout_centerInParent="true"
                                    android:visibility="gone" />
                            </RelativeLayout>

                            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                                android:id="@+id/percentage"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:gravity="center"
                                android:text="0%"
                                android:textColor="@color/whitea"
                                android:textSize="@dimen/ousttext_dimen25" />

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/oustlayout_dimen40"></RelativeLayout>
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/oustlayout_dimen150"
                            android:layout_alignParentBottom="true">

                            <com.oustme.oustsdk.customviews.CustomTextView
                                android:id="@+id/downloadlabel_content"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:textColor="@color/whitelight"
                                android:textSize="@dimen/ousttext_dimen20"
                                android:visibility="visible"
                                tools:text="sdffgfg" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/nointernetconnection_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/oustlayout_dimen40"
                        android:visibility="gone">

                        <com.oustme.oustsdk.customviews.CustomTextView
                            android:id="@+id/nointernetconnection_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:gravity="center_horizontal"
                            android:padding="@dimen/oustlayout_dimen15"
                            android:text=""
                            android:textSize="@dimen/ousttext_dimen23" />
                    </RelativeLayout>
                </RelativeLayout>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/fragement_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="visible" />
        </RelativeLayout>

    </RelativeLayout>
</LinearLayout>