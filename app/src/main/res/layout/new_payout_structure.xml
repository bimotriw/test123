<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/extra_tier_info_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/oustlayout_dimen10"
        android:background="@color/whitea"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/oustlayout_dimen10"
            android:paddingRight="@dimen/oustlayout_dimen10"
            android:paddingTop="@dimen/oustlayout_dimen10"
            android:orientation="horizontal">

            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:paddingLeft="@dimen/oustlayout_dimen5"
                android:paddingRight="@dimen/oustlayout_dimen5"
                android:text="City : "
                android:textColor="@color/common_grey"
                android:textSize="@dimen/ousttext_dimen15" />

            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:id="@+id/city_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bangalore"
                android:textAllCaps="true"
                android:textColor="@color/common_grey"
                android:textSize="@dimen/ousttext_dimen15" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/oustlayout_dimen10"
            android:paddingRight="@dimen/oustlayout_dimen10"
            android:orientation="horizontal">

            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/oustlayout_dimen5"
                android:paddingRight="@dimen/oustlayout_dimen5"
                android:text="Area : "
                android:textColor="@color/common_grey"
                android:textSize="@dimen/ousttext_dimen15" />

            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:id="@+id/area_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAllCaps="true"
                android:textColor="@color/common_grey"
                android:textSize="@dimen/ousttext_dimen15" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/oustlayout_dimen10"
            android:paddingRight="@dimen/oustlayout_dimen10"
            android:orientation="horizontal">

            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/oustlayout_dimen5"
                android:paddingRight="@dimen/oustlayout_dimen5"
                android:text="Job Type : "
                android:textColor="@color/common_grey"
                android:textSize="@dimen/ousttext_dimen15" />

            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:id="@+id/job_type_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAllCaps="true"
                android:textColor="@color/common_grey"
                android:textSize="@dimen/ousttext_dimen15" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/oustlayout_dimen10"
            android:paddingRight="@dimen/oustlayout_dimen10"
            android:orientation="horizontal">

            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/oustlayout_dimen5"
                android:paddingRight="@dimen/oustlayout_dimen5"
                android:text="Shift Time : "
                android:textColor="@color/common_grey"
                android:textSize="@dimen/ousttext_dimen15" />

            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:id="@+id/shift_time_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAllCaps="true"
                android:textColor="@color/common_grey"
                android:textSize="@dimen/ousttext_dimen15" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/oustlayout_dimen1"
            android:background="@color/LiteGray"></View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/oustlayout_dimen10"
            android:paddingRight="@dimen/oustlayout_dimen10"
            android:paddingTop="@dimen/oustlayout_dimen5"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/daily_incentive_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:layout_marginBottom="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen13"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:layout_height="@dimen/oustlayout_dimen13"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="Daily Incentive"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen15" />
                </LinearLayout>
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/common_grey"
                    android:id="@+id/daily_incentive_text"
                    android:paddingLeft="@dimen/oustlayout_dimen35"
                    android:text="Upto Rs 200"
                    android:textSize="@dimen/ousttext_dimen14"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/minGurantee_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginBottom="@dimen/oustlayout_dimen5"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen13"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:layout_height="@dimen/oustlayout_dimen13"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="Minimum Guarantee"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen15" />
                </LinearLayout>
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/common_grey"
                    android:id="@+id/minGurantee_text"
                    android:paddingLeft="@dimen/oustlayout_dimen35"
                    android:text=""
                    android:textSize="@dimen/ousttext_dimen14"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/per_order_earning_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:layout_marginBottom="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen13"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:layout_height="@dimen/oustlayout_dimen13"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="Per Order Earning Range"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen15" />
                </LinearLayout>
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/common_grey"
                    android:id="@+id/per_order_earning_text"
                    android:paddingLeft="@dimen/oustlayout_dimen35"
                    android:text=""
                    android:textSize="@dimen/ousttext_dimen14"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/monthlyIncentive_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:layout_marginBottom="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen13"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:layout_height="@dimen/oustlayout_dimen13"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="Monthly Incentive"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen15" />
                </LinearLayout>
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/common_grey"
                    android:id="@+id/monthlyIncentive_text"
                    android:paddingLeft="@dimen/oustlayout_dimen35"
                    android:text=""
                    android:textSize="@dimen/ousttext_dimen14"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/weekend_incentive_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:layout_marginBottom="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen13"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:layout_height="@dimen/oustlayout_dimen13"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="Weekend Incentive"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen15" />
                </LinearLayout>
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/common_grey"
                    android:id="@+id/weekend_incentive_text"
                    android:paddingLeft="@dimen/oustlayout_dimen35"
                    android:text=""
                    android:textSize="@dimen/ousttext_dimen14"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/weekly_incentive_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:layout_marginBottom="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen13"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:layout_height="@dimen/oustlayout_dimen13"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="Weekly Incentive"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen15" />
                </LinearLayout>
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/common_grey"
                    android:id="@+id/weekly_incentive_text"
                    android:paddingLeft="@dimen/oustlayout_dimen35"
                    android:text=""
                    android:textSize="@dimen/ousttext_dimen14"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/add_info_1_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen13"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:layout_height="@dimen/oustlayout_dimen13"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="Additional Information"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen15" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/add_info_data_ll"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/oustlayout_dimen10"
                    android:paddingLeft="@dimen/oustlayout_dimen13"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/add_info_2_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:layout_marginBottom="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="visible">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen15"
                        android:layout_height="@dimen/oustlayout_dimen15"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="More Info"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen14" />
                </LinearLayout>
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/common_grey"
                    android:id="@+id/add_info_2_text"
                    android:paddingLeft="@dimen/oustlayout_dimen35"
                    android:text=""
                    android:textSize="@dimen/ousttext_dimen14"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/tenure_incentive_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_marginRight="@dimen/oustlayout_dimen5"
                android:layout_marginLeft="@dimen/oustlayout_dimen5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <View
                        android:layout_width="@dimen/oustlayout_dimen13"
                        android:layout_height="@dimen/oustlayout_dimen13"
                        android:layout_marginTop="@dimen/oustlayout_dimen1"
                        android:background="@drawable/circle_common_grey_bg"></View>

                    <com.oustme.oustsdk.customviews.HeavyCustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/oustlayout_dimen10"
                        android:text="Tenure Incentive"
                        android:textColor="@color/common_grey"
                        android:textSize="@dimen/ousttext_dimen15" />
                </LinearLayout>
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/common_grey"
                    android:id="@+id/tenure_incentive_text"
                    android:paddingLeft="@dimen/oustlayout_dimen35"
                    android:text=""
                    android:textSize="@dimen/ousttext_dimen14"
                    />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/accept_button_ll"
        android:visibility="visible"
        android:paddingRight="@dimen/oustlayout_dimen10"
        android:paddingLeft="@dimen/oustlayout_dimen10"
        android:paddingBottom="@dimen/oustlayout_dimen10"
        android:background="@color/whitea">
        <CheckBox
            android:id="@+id/accept_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:buttonTint="@color/lgreen"
            android:text="Accept and Download"
            android:textSize="@dimen/ousttext_dimen16"
            android:textStyle="bold"
            android:gravity="center_vertical"
            android:background="@color/whitea"
            android:padding="@dimen/oustlayout_dimen5"

            />
    </LinearLayout>
</RelativeLayout>