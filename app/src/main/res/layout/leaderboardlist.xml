<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/White">
    <com.oustme.oustsdk.customviews.TryRippleView
        android:id="@+id/list_mainlayout_rippleView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:rv_rippleDuration="250"
        app:rv_color="#01b5a2">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/leaderboardlist_mainlayout">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/oustlayout_dimen5"
            android:layout_centerInParent="true">
            <com.oustme.oustsdk.customviews.CustomTextView
                android:layout_width="@dimen/oustlayout_dimen40"
                android:layout_height="@dimen/oustlayout_dimen40"
                android:textColor="@color/DarkGray"
                android:gravity="center|center_vertical"
                android:textSize="@dimen/ousttext_dimen16"
                tools:text="1"
                android:layout_centerVertical="true"
                android:id="@+id/indexText"/>
            <com.oustme.oustsdk.customviews.CircleImageView
                android:layout_width="@dimen/oustlayout_dimen50"
                android:layout_height="@dimen/oustlayout_dimen50"
                android:textColor="@color/DarkGray"
                android:gravity="left|center_vertical"
                android:background="@drawable/circle_dark_gray"
                android:textSize="@dimen/ousttext_dimen16"
                android:id="@+id/leaderBoardImage"
                android:layout_toRightOf="@+id/indexText"
                android:contentDescription="@string/app_name"
                android:layout_marginLeft="@dimen/oustlayout_dimen10"
                android:layout_marginTop="@dimen/oustlayout_dimen5" />
            <com.oustme.oustsdk.customviews.HeavyCustomTextView
                android:layout_width="@dimen/oustlayout_dimen50"
                android:layout_height="@dimen/oustlayout_dimen50"
                android:textColor="@color/whitea"
                android:visibility="gone"
                android:gravity="center"
                android:maxLength="2"
                tools:text="leaderBoardImageText"
                android:textSize="@dimen/ousttext_dimen15"
                android:id="@+id/leaderBoardImageText"
                android:layout_toRightOf="@+id/leaderBoardImage"
                android:contentDescription="@string/app_name"
                android:layout_marginLeft="@dimen/oustlayout_dimen10"
                android:layout_marginTop="@dimen/oustlayout_dimen5" />
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="@dimen/oustlayout_dimen60"
                android:layout_toLeftOf="@+id/xpValue"
                android:layout_toRightOf="@+id/leaderBoardImageText"
                android:gravity="center_vertical">
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/DarkGray"
                    android:gravity="left|center_vertical"
                    android:textSize="@dimen/ousttext_dimen16"
                    android:id="@+id/name"
                    tools:text="Name"
                    android:layout_marginLeft="@dimen/oustlayout_dimen10"
                    android:singleLine="true"/>

                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/DarkGray"
                    android:gravity="left|center_vertical"
                    android:textSize="@dimen/ousttext_dimen16"
                    android:id="@+id/lbAddInfo"
                    android:text=""
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/oustlayout_dimen2"
                    android:layout_below="@+id/name"
                    android:layout_marginLeft="@dimen/oustlayout_dimen10"
                    android:singleLine="true"/>

                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/gamecountNo"
                    android:layout_below="@+id/name"
                    android:textSize="@dimen/ousttext_dimen8"
                    android:visibility="gone"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/oustlayout_dimen10"
                    android:text=""
                    android:textColor="@color/DarkGray"
                    android:paddingTop="@dimen/oustlayout_dimen1" />
                <com.oustme.oustsdk.customviews.CustomTextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/xpleveltext"
                    android:layout_below="@+id/name"
                    android:layout_marginTop="@dimen/oustlayout_dimen4"
                    android:textSize="@dimen/ousttext_dimen8"
                    android:visibility="gone"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/oustlayout_dimen10"
                    android:text=""
                    android:textColor="@color/DarkGray"
                    android:paddingTop="@dimen/oustlayout_dimen3" />
            </RelativeLayout>
            <com.oustme.oustsdk.customviews.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/oustlayout_dimen50"
                android:textColor="@color/DarkGray"
                android:gravity="center|center_vertical"
                android:layout_marginTop="1dp"
                android:textSize="@dimen/ousttext_dimen16"
                tools:text="Veerendra"
                android:id="@+id/xpValue"
                android:layout_toLeftOf="@+id/victories"
                android:layout_alignParentTop="true" />
            <com.oustme.oustsdk.customviews.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/oustlayout_dimen50"
                android:textColor="@color/DarkGray"
                android:gravity="center"
                android:textSize="@dimen/ousttext_dimen13"
                android:lineSpacingExtra="@dimen/oustlayout_dimen3"
                android:id="@+id/victories"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:visibility="visible"
                android:paddingTop="@dimen/oustlayout_dimen1" />
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0.8dp"
        android:background="@color/LiteGray"
        android:layout_marginRight="@dimen/oustlayout_dimen5"
        android:layout_marginLeft="@dimen/oustlayout_dimen5"
        android:visibility="gone"
        android:layout_below="@+id/leaderboardlist_mainlayout">
    </RelativeLayout>
    </com.oustme.oustsdk.customviews.TryRippleView>
</RelativeLayout>